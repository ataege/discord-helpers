{
  "name": "@crew22/discord-helpers",
  "version": "0.1.0",
  "description": "discord.js helper functions for crew22.",
  "main": "./build/cjs/index.js",
  "module": "./build/esm/index.js",
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:changelog": "npx @discordx/changelog --root=src",
    "build:cjs": "tsc --module CommonJs --outDir build/cjs && npm run postbuild:cjs",
    "postbuild:cjs": "node ./scripts/postbuild.mjs --path=build",
    "build:esm": "tsc",
    "type-check": "tsc --noemit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ataege/discord-helpers.git"
  },
  "keywords": [
    "discord",
    "utility"
  ],
  "exports": {
    ".": {
      "import": "./build/esm/index.js",
      "require": "./build/cjs/index.js"
    }
  },
  "author": "github.com/ataege",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ataege/discord-helpers/issues"
  },
  "homepage": "https://github.com/ataege/discord-helpers#readme",
  "dependencies": {
    "@types/node": "^17.0.21",
    "discord.js": ">=13 || ^13.0.0-dev",
    "glob": "^7.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "typescript": "^4.6.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "discord.js": ">=13 || ^13.0.0-dev"
  }
}
