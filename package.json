{
  "name": "@crew22/discord-helpers",
  "version": "0.1.3",
  "description": "discord.js helper functions for crew22.",
  "main": "./build/cjs/index.js",
  "module": "./build/esm/index.js",
  "files": [
    "build/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ataege/discord-helpers.git"
  },
  "keywords": [
    "discord",
    "utility"
  ],
  "exports": {
    ".": {
      "import": "./build/esm/index.js",
      "require": "./build/cjs/index.js"
    }
  },
  "author": "github.com/ataege",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ataege/discord-helpers/issues"
  },
  "homepage": "https://github.com/ataege/discord-helpers#readme",
  "dependencies": {
    "@types/node": "^17.0.21",
    "discord.js": ">=13 || ^13.0.0-dev",
    "glob": "^7.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.6.0",
    "typescript": "^4.6.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "discord.js": ">=13 || ^13.0.0-dev"
  }
}
